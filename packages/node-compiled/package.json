{
  "name": "@repo/node-compiled",
  "description": "compiled internal package for Node.js",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./math": {
      "types": "./dist/math.d.ts",
      "default": "./dist/math.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "eslint": "^9.17.0",
    "prettier": "^3.6.2",
    "typescript": "^5.7.3"
  }
}
